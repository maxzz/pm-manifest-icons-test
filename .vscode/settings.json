{
  "multiCommand.commands": [
    {
      "command": "multiCommand.openTerminalInNearestPackage",
      "sequence": [
        "workbench.action.terminal.new",
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "for /f \"delims=\" %p in ('node \"${workspaceFolder}\\.vscode\\find-package-dir.js\" \"${file}\"') do cd /d %p\r"
          }
        }
      ]
    }
  ]
}
