{
  "multiCommand.commands": [
    {
      "command": "multiCommand.openTerminalInNearestPackage",
      "sequence": [
        "workbench.action.terminal.new",
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "for /f \"delims=\" %p in ('node \"${workspaceFolder}\\.vscode\\find-package-dir.js\" \"${file}\"') do (cd /d %p & powershell -NoProfile -Command \"Write-Host ([char]27 + ']0;' + '%~nP' + [char]7) -NoNewline\")\r"
          }
        },
        "terminal-helpers.renameNearestPackageTerminal"
      ]
    }
  ]
}
